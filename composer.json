{
    "name": "app/template-admin-php",
    "description": "Template administrativo PHP + PostgreSQL + Twig com login e RBAC",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "twig/twig": "^3.0",
        "vlucas/phpdotenv": "^5.6",
        "nikic/fast-route": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
